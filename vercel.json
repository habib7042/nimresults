{
  "buildCommand": "npx prisma generate && next build",
  "outputDirectory": ".next",
  "installCommand": "npm install && npx prisma generate",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}